<div class="container-xxl flex-grow-1 container-p-y">
    <h4 class="fw-bold py-3 mb-4">CommentNews</h4>
<div class="card mb-4">
    <h5 class="card-header"><strong>User</strong></h5>
    <div class="card-body">
        <div class="mb-3 row">
            <label for="html5-text-input" class="col-md-2 col-form-label">@Html.DisplayNameFor(model => model.CommentNews.Content)</label>
            <div class="col-md-10">
                <p class="form-control" style="margin-bottom:0px">@Html.DisplayFor(model => model.CommentNews.Content)</p>
            </div>
        </div>
        <div class="mb-3 row">
            <label for="html5-text-input" class="col-md-2 col-form-label"> @Html.DisplayNameFor(model => model.CommentNews.CreatedDay)</label>
            <div class="col-md-10">
                <p class="form-control" style="margin-bottom:0px">@Html.DisplayFor(model => model.CommentNews.CreatedDay)</p>
            </div>
        </div>
        <div class="mb-3 row">
            <label for="html5-text-input" class="col-md-2 col-form-label"> @Html.DisplayNameFor(model => model.CommentNews.UpdateDay)</label>
            <div class="col-md-10">
                <p class="form-control" style="margin-bottom:0px">@Html.DisplayFor(model => model.CommentNews.UpdateDay)</p>
            </div>
        </div>
        <div class="mb-3 row">
            <label for="html5-text-input" class="col-md-2 col-form-label"> @Html.DisplayNameFor(model => model.CommentNews.CreatedByNavigation)</label>
            <div class="col-md-10">
                <p class="form-control" style="margin-bottom:0px">@Html.DisplayFor(model => model.CommentNews.CreatedByNavigation.FullName)</p>
            </div>
        </div>
        <div class="mb-3 row">
            <label for="html5-text-input" class="col-md-2 col-form-label"> @Html.DisplayNameFor(model => model.CommentNews.News)</label>
            <div class="col-md-10">
                <p class="form-control" style="margin-bottom:0px">@Html.DisplayFor(model => model.CommentNews.News.NewId)</p>
            </div>
        </div>
        <div class="mb-3 row">
            <label for="html5-text-input" class="col-md-2 col-form-label"> @Html.DisplayNameFor(model => model.CommentNews.UpdateByNavigation)</label>
            <div class="col-md-10">
                <p class="form-control" style="margin-bottom:0px">@Html.DisplayFor(model => model.CommentNews.UpdateByNavigation.FullName)</p>
            </div>
        </div>
        <div class="mb-3 row">
            <div class="mb-3">
                <button type="button" class="btn btn-primary"> <a asp-page="./Edit" asp-route-id="@Model.CommentNews.CmtNewsId" style="color:white">Edit</a></button>
            </div>
            <div class="mb-3">
                <button type="button" class="btn btn-secondary"> <a asp-page="./Index" style="color:white">Back to List</a></button>
            </div>
        </div>
    </div>
</div>
</div>